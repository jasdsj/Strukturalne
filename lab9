#include<stdio.h>
#include<stdlib.h>

int** alokuj(unsigned int n, unsigned int m){

int **t=malloc(n*sizeof(int*));
int i;
for(i=0;i<n;i++)
{
    t[i]=malloc(m*sizeof(int));
}
return t;
}

int(* alokuj2(unsigned int n, unsigned int m))[]{
    return malloc(n* sizeof(int[m]));
}

void wypisz(int **t, unsigned int n, unsigned int m){

int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        printf("%d\t", t[i][j]);
    }
printf("\n");
}

}

void wypisz2( unsigned int n, unsigned int m, int t[][m]){
int i, j;
for (i=0;i<n;i++)
{
    for(j=0;j<n;j++)
    {
        printf("%d\t", t[i][j]);
    }
printf("\n");
}

}

void wczytaj(int **t, unsigned int n, unsigned int m){

int i, j;
for (i=0;i<n;i++)
{
    for(j=0;j<n;j++)
    {
      scanf("%d", &t[i][j]);
    }
printf("\n");
}
}

void wczytaj2(unsigned int n, unsigned int m, int t[][m]){

int i, j;
for (i=0;i<n;i++)
{
    for(j=0;j<n;j++)
    {
      scanf("%d", &t[i][j]);
    }
printf("\n");
}
}


void zwolnij(unsigned int n, unsigned int m, int **t){

int i;
for (i=0;i<n;i++)
{
    free(t[i]);
}
free(t);
}

int** alokujtrd(unsigned int n, unsigned int m){

int **t=malloc(n*sizeof(int*));
int i;
for(i=0;i<n;i++)
{
    t[i]=malloc((i+1)*sizeof(int));
}
return t;
}

int** alokujtrg(unsigned int n, unsigned int m){

int **t=malloc(n*sizeof(int*));
int i;
for(i=0;i<n;i++)
{
    t[i]=malloc((m-i)*sizeof(int));
}
return t;
}

void zeruj(int **t, unsigned int n, unsigned int m){

int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        t[i][j] = 0;
    }
printf("\n");


}}

int sumuj(int **t, unsigned int n, unsigned int m){
int temp = 0;
int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        temp += t[i][j];
    }
}return temp;}


int najw(int **t, unsigned int n, unsigned int m){
int max = 0;
int sum = 0;
int ind = 0;
int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        sum += t[i][j];
    }
    if (sum > max){
        max = sum;
        ind = i;
        sum = 0;
    } else
    {
        sum = 0;
    }
}return ind;}

int zera(int **t, unsigned int n, unsigned int m){
int temp = 0;
int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        if ( t[i][j] == 0){
            temp+= 1;
        }
    }
}return temp;}

void przepisz(int **t, int  **r, unsigned int n, unsigned int m){

int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
       r[i][j] = t[i][j];

    }
}
}

void zamien(int **t, int  **r, unsigned int n, unsigned int m){

int temp, i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        temp = r[i][j];
        r[i][j]= t[i][j];
        t[i][j]=temp;
    }
}
}

void odwroc(int **t, unsigned int n, unsigned int m){
int **r=alokuj(3,3);
przepisz(t,r,3,3);
int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
       t[i][j] = r[i][m-j-1];

    }
}
}


void dol(int **t, unsigned int n, unsigned int m){
int **r=alokuj(3,3);
przepisz(t,r,3,3);
int i, j;
for (i=0;i<n;i++)
{
    for (j=0;j<n;j++)
    {
        if (i == 0){
        t[i][j] = r[n-1][j];
       }
       else{
        t[i][j] = r[i-1][j];
       }

    }
}
}

int main(){

int **t=alokuj(3,3);
wczytaj(t,3,3);
dol(t,3,3);
wypisz(t,3,3);

/*

int t2[3][3];
wczytaj2(3,3,t2);
wypisz2(3,3,t2);*/




return 0;
}

main();
